#include <NTL/ZZ.h>
#include <iostream>
using namespace std;
using namespace NTL;
int main()
{
   ZZ acc, val;
   acc = 0;
   while (SkipWhiteSpace(cin)) {
      cin >> val;
      if(val==1)
        break;
      acc += val*val;
   }

   cout << acc << "\n";
}
